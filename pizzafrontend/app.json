{
  "expo": {
    "name": "Friends Pizza Hut",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.naitik2408.boltexponativewind"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo.png",
          "color": "#FF6B00",
          "sounds": [
            "./assets/notification_sound.wav"
          ],
          "androidMode": "default",
          "androidCollapsedTitle": "Friends Pizza Hut",
          "iosDisplayInForeground": true,
          "androidChannels": [
            {
              "id": "order_alerts",
              "name": "Order Alerts",
              "description": "Critical notifications for new orders - System Level Alerts",
              "priority": "max",
              "vibrationPattern": [
                0,
                1500,
                300,
                1500,
                300,
                1500
              ],
              "sound": "notification_sound.wav",
              "enableLights": true,
              "lightColor": "#FF6B00",
              "lockscreenVisibility": "public",
              "bypassDnd": true,
              "importance": "max",
              "showBadge": true
            },
            {
              "id": "full_screen_alerts",
              "name": "Full Screen Order Alerts",
              "description": "Call-like alerts for new orders",
              "priority": "max",
              "vibrationPattern": [
                0,
                2000,
                500,
                2000,
                500,
                2000
              ],
              "sound": "notification_sound.wav",
              "enableLights": true,
              "lightColor": "#FF0000",
              "lockscreenVisibility": "public",
              "bypassDnd": true,
              "importance": "max",
              "showBadge": true
            }
          ]
        }
      ],
      [
        "./plugins/withRazorpay.js"
      ],
      [
        "./plugins/withFirebaseMessaging.js"
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "27cd6137-1783-482b-8dd0-571a3dc859f2"
      }
    },
    "android": {
      "package": "com.naitik2408.boltexponativewind",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "NOTIFICATIONS",
        "ACCESS_NOTIFICATION_POLICY",
        "POST_NOTIFICATIONS",
        "WAKE_LOCK",
        "USE_FULL_SCREEN_INTENT",
        "SYSTEM_ALERT_WINDOW",
        "DISABLE_KEYGUARD",
        "TURN_SCREEN_ON",
        "SHOW_WHEN_LOCKED",
        "REQUEST_IGNORE_BATTERY_OPTIMIZATIONS"
      ]
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}
