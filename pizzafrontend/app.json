{
  "expo": {
    "name": "Friends Pizza Hut",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.naitik2408.boltexponativewind"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo.png",
          "color": "#FF6B00",
          "sounds": [
            "./assets/notification_sound.wav"
          ],
          "androidMode": "default",
          "androidCollapsedTitle": "Friends Pizza Hut",
          "iosDisplayInForeground": true
        }
      ],
      [
        "./plugins/withRazorpay.js"
      ],
      [
        "./plugins/withFirebaseMessaging.js"
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "27cd6137-1783-482b-8dd0-571a3dc859f2"
      }
    },
    "android": {
      "package": "com.naitik2408.boltexponativewind",
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "NOTIFICATIONS",
        "ACCESS_NOTIFICATION_POLICY",
        "POST_NOTIFICATIONS"
      ]
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}